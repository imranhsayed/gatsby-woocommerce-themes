{
	"name": "site",
	"version": "2.1.0",
	"description": "Site to test the Gatsby WooCommerce Theme",
	"main": "gatsby-config.js",
	"scripts": {
		"build": "gatsby build",
		"build:clean": "npm run clean && npm run build",
		"dev": "gatsby develop",
		"dev:clean": "npm run clean && npm run dev",
		"clean": "gatsby clean",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"start": "npm run dev",
		"serve": "gatsby serve",
		"test": "jest",
		"test:debug": "jest --runInBand",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage=true",
		"test:update": "jest --verbose --updateSnapshot"
	},
	"author": "Imran Sayed",
	"license": "MIT",
	"dependencies": {
		"dotenv": "^10.0.0",
		"gatsby": "^3.8.1",
		"mini-css-extract-plugin": "0.4.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"resolutions": {
		"graphql": "^15.4.0",
		"graphql-compose": "^7.25.0",
		"webpack": "^5.24.2"
	},
	"devDependencies": {}
}
